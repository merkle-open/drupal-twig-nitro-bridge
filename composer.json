{
  "name": "namics/drupal-twig-nitro-bridge",
  "description": "Provides integration for the Nitro Twig library within Drupal.",
  "type": "drupal-module",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/namics/drupal-twig-nitro-bridge/issues",
    "source": "https://github.com/namics/drupal-twig-nitro-bridge"
  },
  "keywords": [
    "Drupal",
    "Twig",
    "Nitro"
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "namics/twig-nitro-library",
        "version": "1.0.0",
        "dist": {
          "url": "https://github.com/namics/twig-nitro-library/archive/v1.0.0.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "namics/twig-nitro-library": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\twig_nitro_bridge\\": "src/"
    }
  }
}
